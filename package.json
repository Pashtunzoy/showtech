{
  "name": "showtech",
  "version": "1.0.0",
  "description": "An app for event ticketing using Stripe",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node ./tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress ./tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Asif Norzai",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-register": "^6.11.6",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "nodemon": "^1.10.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "stripe": "^4.9.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
